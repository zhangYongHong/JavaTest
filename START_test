import  java.io.*;
import java.util.jar.Attributes.Name;

public class hello extends Thread  {
	public hello(){
	
	}
	public hello(String name){
		 this.name = name;
	}
	
	 public void run(){
		 for(int i = 0;  i < 5;  i++)
			 System.out.println(name  + "运行    " + i);
	 }
	 
	public static void main(String[] args) {
	    hello h1 = new hello("A");
	    hello h2 = new hello("B");
	    h1.start();
	    h2.start();
	}

	private String name;
}

result：
A运行    0
A运行    1
A运行    2
B运行    0
A运行    3
B运行    1
A运行    4
B运行    2
B运行    3
B运行    4
